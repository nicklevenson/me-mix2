<% flash.each do |type, msg| %>
  <div class="alert alert-info">
    <%= msg %>
  </div>
<% end %>
<%=form_with url: '/login', method: :post do |f|%>
  <%=f.label :email %>
  <%=f.text_field :email%>
  <%=f.label :password%>
  <%=f.password_field :password%>
  <%=f.submit 'Login'%>
<%end%>
<%= button_to 'Login with Google', '/auth/google_oauth2' %>

<%=button_to "Log in with Facebook",'/auth/facebook'%>