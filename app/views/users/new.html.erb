<h3>Feeling mixed up? Create an account!</h3>
<%=render partial: 'errors', locals: {user: @user}%>
  <%=form_for @user do |f|%>
    <%=f.label :username%>
    <%=f.text_field :username%> <br>
    <%=f.label :email%>
    <%=f.text_field :email%> <br>
    <%=f.label :password%>
    <%=f.password_field :password%> <br>
    <%=f.label :password_confirmation%>
    <%=f.password_field :password_confirmation%> <br>
    <%=f.submit%>
  <%end%>
